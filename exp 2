# Input data
age <- c(13, 15, 16, 16, 19, 20, 20, 21, 22, 22, 25, 25, 25, 25, 30, 
         33, 33, 35, 35, 35, 35, 36, 40, 45, 46, 52, 70)

# Compute mean
mean_age <- mean(age)

# Compute median
median_age <- median(age)

# Compute mode
mode_age <- as.numeric(names(table(age))[table(age) == max(table(age))])

# Print results
cat("Mean:", mean_age, "\n")
cat("Median:", median_age, "\n")
cat("Mode:", mode_age, "\n")
