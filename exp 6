# Input
age <- c(23, 23, 27, 27, 39, 41, 47, 49, 50, 52, 54, 54, 56, 57, 58, 58, 60, 61)
value <- 35

# Min-Max Normalization
min_max <- (value - min(age)) / (max(age) - min(age))

# Z-Score Normalization
z_score <- (value - mean(age)) / 12.94

# Decimal Scaling
decimal_scaling <- value / 100

cat("Min-Max:", min_max, "\nZ-Score:", z_score, "\nDecimal Scaling:", decimal_scaling, "\n")
